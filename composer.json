{
  "name": "acmarche/themewp",
  "authors": [
    {
      "name": "jfsenechal",
      "email": "jfsenechal@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.2",
    "ext-intl": "*",
    "guzzlehttp/guzzle": "^7.10",
    "meilisearch/meilisearch-php": "^v1.16.0",
    "nesbot/carbon": "^3.10.0",
    "symfony/cache": "^7.3",
    "symfony/console": "^7.3",
    "symfony/dotenv": "^7.3",
    "symfony/finder": "^7.3",
    "symfony/http-client": "^7.3",
    "symfony/http-foundation": "^7.3",
    "symfony/mailer": "^7.3",
    "twig/intl-extra": "^v3.8.0",
    "twig/string-extra": "^v3.8.0",
    "twig/twig": "^v3.8.0",
    "ext-pdo": "*"
  },
  "autoload": {
    "psr-4": {
      "AcMarche\\Theme\\": "wp-content/themes/marchebe/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "allow-plugins": {
      "php-http/discovery": true
    }
  },
  "extra": {
    "symfony": {
      "allow-contrib": true,
      "require": "7.3.*",
      "docker": false
    }
  },
  "require-dev": {
    "symfony/error-handler": "7.3.*"
  }
}
