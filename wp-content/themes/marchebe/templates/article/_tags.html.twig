<div class="flex flex-row flex-wrap gap-2 mt-6">
    {% for tag in tags %}
        <a
                href="{{ tag.url }}"
                class="inline-block bg-blue-100 text-cta-brown text-xs font-semibold px-3 py-1 rounded-full uppercase  hover:bg-blue-200 transition-colors">
            {{ tag.name }}
        </a>
    {% endfor %}
</div>